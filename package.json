{
  "name": "webflow-code-components",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "gsap": "^3.13.0",
    "motion": "^12.23.24",
    "ogl": "^1.0.11",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-scripts": "5.0.1",
    "recharts": "^3.6.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "publish": "npm run publish:limeknight",
    "publish:limeknight": "cp .env.limeknight .env && npm run build && echo 'Y' | npx webflow library share --no-input",
    "publish:studiotonic": "cp .env.studiotonic .env && npm run build && echo 'Y' | npx webflow library share --no-input",
    "publish:both": "npm run publish:limeknight && npm run publish:studiotonic"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@julianmemberstack/webflow-code-components-helper": "^1.0.0",
    "@types/react": "^19.2.2",
    "@webflow/data-types": "^1.1.0",
    "@webflow/react": "^1.1.0",
    "@webflow/webflow-cli": "^1.10.0"
  }
}
